@model IEnumerable<CustomerAPP.Models.CustomersPhoneModel>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CpPhone)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CpPhone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CpId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CpId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CpId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
